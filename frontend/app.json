{
  "expo": {
    "name": "pubStore",
    "slug": "pubStore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-chopp.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pubStore"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-chopp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.pubStore"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
      // "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-chopp.png",
          "imageWidth": 200,
          "contentFit": "cover",
          "backgroundColor": "#fff"
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./assets/images/"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
